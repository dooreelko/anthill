{
    "name": "actually-cloud-native-dorc",
    "version": "1.0.0",
    "description": "Actually cloud-native container orchestrator",
    "main": "dist/main.js",
    "scripts": {
        "get": "cdktf get",
        "build": "tsc && npm run post-build",
        "post-build": " find ./src -iname '*dockerfile' -exec cp '{}' './dist/{}' ';'",
        "synth": "cdktf synth",
        "compile": "tsc --pretty && npm run post-build",
        "watch": "tsc -w",
        "test": "jest",
        "test:watch": "jest --watch",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade:next": "npm i cdktf@next cdktf-cli@next"
    },
    "keywords": [
        "docker",
        "cloud"
    ],
    "author": "dooreelko",
    "license": "GPL-3.0",
    "engines": {
        "node": ">=14.0"
    },
    "dependencies": {
        "@cdktf/provider-docker": "^0.8.36",
        "@cdktf/provider-null": "^0.7.35",
        "cdktf": "^0.11.2",
        "constructs": "^10.1.48",
        "dockerode": "^3.3.2",
        "koa": "^2.13.4",
        "koa-body": "^5.0.0",
        "koa-logger": "^3.2.1",
        "node-fetch": "2.6.7",
        "pixl-json-stream": "^1.0.7"
    },
    "devDependencies": {
        "@tsconfig/node18": "^1.0.1",
        "@types/dockerode": "^3.3.9",
        "@types/jest": "^28.1.6",
        "@types/koa": "^2.13.5",
        "@types/koa__router": "^8.0.11",
        "@types/koa-logger": "^3.1.2",
        "@types/node": "^18.0.5",
        "@types/node-fetch": "^2.6.2",
        "jest": "^28.1.3",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}