{
    "name": "@arinoto/actually-cloud-native-dorc",
    "version": "1.1.0",
    "private": "true",
    "description": "Actually cloud-native container orchestrator",
    "main": "dist/src/main.js",
    "scripts": {
        "get": "cdktf get",
        "build": "tsc && npm run post-build:npm",
        "post-build:npm": "lerna bootstrap --scope ${npm_package_name} -- --omit=dev && cp --recursive --dereference node_modules dist || lerna bootstrap --scope ${npm_package_name}",
        "synth": "cdktf synth",
        "deploy": "cdktf deploy --auto-approve",
        "compile": "tsc --pretty && npm run post-build",
        "watch": "tsc --watch --pretty && npm run post-build",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade:next": "npm i cdktf@next cdktf-cli@next",
        "vis:arch": "vis tsconfig.json architecture.ts > dorc-arch.svg",
        "vis": "vis tsconfig.json > dorc.svg"
    },
    "keywords": [
        "docker",
        "cloud"
    ],
    "author": "dooreelko",
    "license": "LGPL-3.0",
    "engines": {
        "node": ">=14.0"
    },
    "dependencies": {
        "@arinoto/core": "*",
        "@arinoto/local-docker": "*",
        "@cdktf/provider-docker": "^0.8.36",
        "@cdktf/provider-null": "^0.7.35",
        "cdktf": "^0.11.2",
        "constructs": "^10.1.48"
    },
    "devDependencies": {
        "@arinoto/vis": "*"
    }
}