{
    "name": "@anthill/actually-cloud-native-dorc",
    "version": "1.0.0",
    "description": "Actually cloud-native container orchestrator",
    "main": "dist/src/main.js",
    "scripts": {
        "get": "cdktf get",
        "build": "tsc && npm run post-build:docker && npm run post-build:npm",
        "post-build:docker": "find ./src -iname '*dockerfile' -exec cp '{}' './dist/{}' ';'",
        "post-build:npm": "lerna bootstrap --scope ${npm_package_name} -- --omit=dev && cp --recursive --dereference node_modules dist && cp package.json dist",
        "synth": "cdktf synth",
        "compile": "tsc --pretty && npm run post-build",
        "watch": "tsc --watch --pretty && npm run post-build",
        "test": "jest",
        "test:watch": "jest --watch",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade:next": "npm i cdktf@next cdktf-cli@next"
    },
    "keywords": [
        "docker",
        "cloud"
    ],
    "author": "dooreelko",
    "license": "LGPL-3.0",
    "engines": {
        "node": ">=14.0"
    },
    "dependencies": {
        "@anthill/core": "*",
        "@cdktf/provider-docker": "^0.8.36",
        "@cdktf/provider-null": "^0.7.35",
        "cdktf": "^0.11.2",
        "constructs": "^10.1.48",
        "dockerode": "^3.3.2",
        "koa": "^2.13.4",
        "koa-body": "^5.0.0",
        "koa-logger": "^3.2.1",
        "node-fetch": "2.6.7",
        "pixl-json-stream": "^1.0.7"
    },
    "devDependencies": {
        "@types/dockerode": "^3.3.9",
        "@types/koa": "^2.13.5",
        "@types/koa__router": "^8.0.11",
        "@types/koa-logger": "^3.1.2",
        "@types/node-fetch": "^2.6.2"
    }
}